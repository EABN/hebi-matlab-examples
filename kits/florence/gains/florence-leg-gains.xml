<?xml version="1.0" encoding="utf-8"?>
<!-- GAINS FOR ONE 6-DOF LEG OF FLORENCE (BIPED-KIT) -->
<!-- The code that loads these gains copy them over to make them the    -->
<!-- the right size for the whole robot.  There are gains for 8 modules -->
<!-- because the Hip3 and Knee DoFs are doubled up.                     --> 
<!--                                                                    -->
<!-- Gains go from hip-to-ankle:                                        -->
<!-- Hip1 -> Hip2 -> Hip3A / Hip3B -> KneeA / KneeB -> Ankle1 -> Ankle2 -->
<!--                                                                    -->
<!-- Dave Rollinson                                                     -->
<!-- Feb 2019                                                           -->

<group_gains>
   <control_strategy>4 4 4 4 4 4 4 4</control_strategy>
   
   <position>
      <kp>30 15 20 20 25 25 5 5</kp>
      <ki>0 0 0 0 0 0 0 0</ki>
      <kd>0 0 0 0 0 0 0 0</kd>
      <feed_forward>0 0 0 0 0 0 0 0</feed_forward>
      <dead_zone>0 0 0 0 0 0 0 0</dead_zone>
      <i_clamp>1 3 10 10 10 10 5 2</i_clamp>
      <punch>0 0 0 0 0 0 0 0</punch>
      <min_target>-inf -inf -inf -inf -inf -inf -inf -inf</min_target>
      <max_target>inf inf inf inf inf inf inf inf</max_target>
      <target_lowpass>1 1 1 1 1 1 1 1</target_lowpass>
      <min_output>-25 -40 -40 -40 -40 -40 -25 -25</min_output>
      <max_output>25 40 40 40 40 40 25 25</max_output>
      <output_lowpass>1 1 1 1 1 1 1 1</output_lowpass>
      <d_on_error>1 1 1 1 1 1 1 1</d_on_error>
   </position>
   
   <velocity>
      <kp>0.01 0.01 0.02 0.02 0.02 0.02 0.02 0.01</kp>
      <ki>0 0 0 0 0 0 0 0</ki>
      <kd>0 0 0 0 0 0 0 0</kd>
      <feed_forward>1 1 1 1 1 1 1 1</feed_forward>
      <dead_zone>0 0 0 0 0 0 0 0</dead_zone>
      <i_clamp>0.25 0.25 0.25 0.25 0.25 0.25 0.25 0.25</i_clamp>
      <punch>0 0 0 0 0 0 0 0</punch>
      <min_target>-3.43 -1.79 -1.79 -1.79 -1.79 -1.79 -3.43 -3.43</min_target>
      <max_target>3.43 1.79 1.79 1.79 1.79 1.79 3.43 3.43</max_target>
      <target_lowpass>1 1 1 1 1 1 1 1</target_lowpass>
      <min_output>-1 -1 -1 -1 -1 -1 -1 -1</min_output>
      <max_output>1 1 1 1 1 1 1 1</max_output>
      <output_lowpass>0.75 0.75 0.75 0.75 0.75 0.75 0.75 0.75</output_lowpass>
      <d_on_error>1 1 1 1 1 1 1 1</d_on_error>
   </velocity>
   
   <effort>
      <kp>0.1 0.1 0.2 0.2 0.2 0.2 0.1 0.1</kp>
      <ki>0 0 0 0 0 0 0 0</ki>
      <kd>0.0001 0.0001 0.0002 0.0002 0.0002 0.0002 0.0001 0.0001</kd>
      <feed_forward>1 1 1 1 1 1 1 1</feed_forward>
      <dead_zone>0 0 0 0 0 0 0 0</dead_zone>
      <i_clamp>0.25 0.25 0.25 0.25 0.25 0.25 0.25 0.25</i_clamp>
      <punch>-0 -0 -0 -0 -0 -0 -0 -0</punch>
      <min_target>-25 -40 -40 -40 -40 -40 -25 -25</min_target>
      <max_target>25 40 40 40 40 40 25 25</max_target>
      <target_lowpass>1 1 1 1 1 1 1 1</target_lowpass>
      <min_output>-1 -1 -1 -1 -1 -1 -1 -1</min_output>
      <max_output>1 1 1 1 1 1 1 1</max_output>
      <!-- <output_lowpass>0.15 0.15 0.15 0.15 0.15 0.15 0.15 0.15</output_lowpass> -->
      <output_lowpass>0.9 0.9 0.9 0.9 0.9 0.9 0.9 0.9</output_lowpass>
      <d_on_error>0 0 0 0 0 0 0 0</d_on_error>
   </effort>
   
</group_gains>